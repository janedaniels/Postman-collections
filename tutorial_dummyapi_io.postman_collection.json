		{
			"name": "Create user_pozitive",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();\r",
							"pm.environment.set(\"email\", jsonData.email);\r",
							"pm.environment.set(\"userId\", jsonData.id)\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"id\");\r",
							"});\r",
							"pm.test(\"Check for digits in firstName\", function () {\r",
							"    var firstName = pm.response.json().firstName;\r",
							"    pm.expect(firstName).to.not.match(/\\d/);\r",
							"});\r",
							"pm.test(\"Check for digits in firstName\", function () {\r",
							"    var firstName = pm.response.json().firstName;\r",
							"    var containsDigits = /\\d/.test(firstName);\r",
							"    pm.expect(containsDigits).to.be.false;\r",
							"});\r",
							"pm.test(\"Check for invalid characters in firstName\", function () {\r",
							"    var firstName = pm.response.json().firstName;\r",
							"    var containsInvalidCharacters = /[^A-Za-z\\-]/.test(firstName);\r",
							"    pm.expect(containsInvalidCharacters).to.be.false;\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
